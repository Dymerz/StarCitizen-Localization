    "@types/ini": "^1.3.31",
    "@types/node": "^20.8.6",
    "commander": "^11.1.0",
    "ini": "^4.1.1",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1"
  },
  "devDependencies": {
  }
}

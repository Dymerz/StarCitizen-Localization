  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:merge": "ts-node src/index.ts merge",
    "watch:merge": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"npx ts-node src/index.ts merge\"",
    "watch:validate": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"npx ts-node src/index.ts validate ./examples/base.ini ./examples/test.ini\"",
    "build": "tsc -p ."
  },
